    <Grid Loaded="PageLoaded" x:Name="sampleGrid" KeyDown="Viewer_KeyDown">
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Background="Gray">
            <cc:SfPdfViewerControl Name="pdfViewer"></cc:SfPdfViewerControl>
        </Grid>

        <Grid Grid.Row="1"  VerticalAlignment="Bottom" BorderBrush="#262726" Margin="0,-2,0,0" Background="#262726" Tapped="Grid_Tapped" IsTapEnabled="False">
            <Grid x:Name="bottomTab">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="1*"></ColumnDefinition>
                    <ColumnDefinition Width="0.5*"></ColumnDefinition>
                </Grid.ColumnDefinitions>

                <Grid Grid.Column="0" Name="FirstGrid">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="2*"></ColumnDefinition>
                        <ColumnDefinition Width="2*"></ColumnDefinition>
                        <ColumnDefinition Width="2*"></ColumnDefinition>
                        <ColumnDefinition Width="0.5*"></ColumnDefinition>
                        <ColumnDefinition Width="2*"></ColumnDefinition>
                        <ColumnDefinition Width="2*"></ColumnDefinition>
                        <ColumnDefinition Width="2*"></ColumnDefinition>
                        <ColumnDefinition Width="0.5*"></ColumnDefinition>
                        <ColumnDefinition Width="2*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <Grid Grid.Column="0" HorizontalAlignment="Center" >
                        <Button Command="{Binding NextPageCommand}" Style="{StaticResource PdfViewerNavigationDownStyle}" Tag="NextPage" Name="NextPageButton"/>
                    </Grid>
                    <Grid Grid.Column="1" HorizontalAlignment="Center" >
                        <Button Command="{Binding PreviousPageCommand}" Style="{StaticResource PdfViewerNavigationUpStyle}" Tag="Prev" Name="PrevPageButton" IsEnabled="False"/>
                    </Grid>
                    <Grid Grid.Column="2" HorizontalAlignment="Center" >
                        <Button Style="{StaticResource PdfViewerGoToStyle}" Tag="Go To" Click="GotoPopUpButton_Click"/>
                        <Popup IsOpen="False" Name="GotoPagePopUp">
                            <Grid Margin="-62,-50,0,0" Width="Auto">
                                <Border Background="#FFEAEAEA" BorderBrush="#FFCECECE" BorderThickness="1.5">
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="2*" />
                                            <ColumnDefinition Width="2*" />
                                            <ColumnDefinition Width="2*" />
                                            <ColumnDefinition Width="2*" />
                                            <ColumnDefinition Width="2*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5,0,0,0">
                                            <TextBox Name="PageDextTxtBox" FontFamily="Segoe UI" FontSize="13" Foreground="#FF0E0E0E" Text="Page Number" GotFocus="PageDextTxtBox_GotFocus" KeyDown="PageDextTxtBox_KeyDown" TextChanged="PageDextTxtBox_TextChanged" Width="110"></TextBox>
                                        </Grid>
                                        <Grid Grid.Column="2" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="5,0,0,0">
                                            <Button Style="{StaticResource OKButtonStyle}" Click="OKButton_Click" BorderThickness="0" Background="Transparent">
                                            </Button>
                                        </Grid>
                                        <Grid Grid.Column="3" Margin="5,0,5,0" Background="Transparent" Tapped="ClosePopUp" BorderThickness="0" HorizontalAlignment="Left" VerticalAlignment="Center" >
                                            <Button Style="{StaticResource CancelButtonStyle}" Background="Transparent">
                                            </Button>
                                        </Grid>
                                    </Grid>
                                </Border>
                            </Grid>
                        </Popup>
                    </Grid>
                    <Rectangle Grid.Column="3" Height="40" Width="1" Margin="0,-12,0,0" Fill="White"/>
                    <Grid Grid.Column="4" HorizontalAlignment="Center" >
                        <Button Command="{Binding IncreaseZoomCommand}" Style="{StaticResource PdfViewerZoomInStyle}" Tag="ZoomIn" />
                    </Grid>
                    <Grid Grid.Column="5" HorizontalAlignment="Center" >
                        <Button Command="{Binding DecreaseZoomCommand}" Style="{StaticResource PdfViewerZoomOutStyle}" Tag="ZoomOut" />
                    </Grid>
                    <Grid Grid.Column="6" HorizontalAlignment="Center" Loaded="ViewModeGrid_Loaded">
                        <Button Style="{StaticResource ViewModeStyle}" Name="ViewModeButton" Tag="View mode" Click="ViewModePopUp_ButtonClick" FlyoutBase.AttachedFlyout="{StaticResource ViewModeFlyout}"/>
                    </Grid>
                    <Rectangle Grid.Column="7" Height="40" Width="1" Margin="0,-12,0,0" Fill="White"/>
                    <Grid Grid.Column="8" HorizontalAlignment="Center">
                        <Button Style="{StaticResource AnnotationStyle}" Name="AnnotationButton" Tag="Annotation" Click="AnnotationButton_Click"/>
                    </Grid>
                </Grid>
                <Grid Grid.Column="1" Name="SecondGrid">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="2*"></ColumnDefinition>
                        <ColumnDefinition Width="2*"></ColumnDefinition>
                        <ColumnDefinition Width="2*"></ColumnDefinition>
                        <ColumnDefinition Width="2*"></ColumnDefinition>
                        <ColumnDefinition Width="2*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <Grid Grid.Column="1" HorizontalAlignment="Center" >
                        <Button x:Name="Open" Style="{StaticResource PdfViewerOpenBtnStyle}" Tag="Open" Click="Open_Click"/>
                    </Grid>
                    <Grid Grid.Column="2" HorizontalAlignment="Center">
                        <Button x:Name="SaveDocument" Style="{StaticResource SaveStyle}" Tag="Save" Click="SaveButton_Click"/>
                    </Grid>
                    <Grid Grid.Column="3" HorizontalAlignment="Center">
                        <Button BorderBrush="White" x:Name="PrintDocument" Style="{StaticResource PdfViewerPrintStyle}"  Tag="Print" Command="{Binding PrintCommand}"/>
                    </Grid>
                    <Grid Grid.Column="4" HorizontalAlignment="Center" >
                        <Button Name="SearchButton" Visibility="Visible" Click="SearchButton_Click" Style="{StaticResource PdfViewerSearchStyle}" Tag="Search" />
                    </Grid>
                </Grid>
            </Grid>
        </Grid>

        <Grid Grid.Row="2"  VerticalAlignment="Bottom"  Background="#262726"  Margin="0,-2,0,0">
            <Grid x:Name="bottomSearchTab" Visibility="Collapsed">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="4.0*"></ColumnDefinition>
                    <ColumnDefinition Width="5.0*"></ColumnDefinition>
                </Grid.ColumnDefinitions>

                <Grid Grid.Column="0" >
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="8*"></ColumnDefinition>
                        <ColumnDefinition Width="4*"></ColumnDefinition>
                        <ColumnDefinition Width="8*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>

                    <Grid Grid.Column="0" HorizontalAlignment="Stretch" >
                        <Grid  VerticalAlignment="Center">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="8*"></ColumnDefinition>
                                <ColumnDefinition Width="Auto"></ColumnDefinition>
                            </Grid.ColumnDefinitions>

                            <Grid Grid.Column="0" Margin="20,0,0,0">
                                <TextBox Height="10" BorderThickness="0" Name="PageSearchTxtBox" FontFamily="Segoe UI" FontSize="15" GotFocus="PageSearchTxtBox_GotFocus" KeyDown="PageSearchTxtBox_KeyDown" />
                            </Grid>

                            <Grid Grid.Column="1">
                                <Button x:Name="SearchIcon" Style="{StaticResource PdfViewerSearchIconStyle}" Tag="SearchIcon" Click="SearchIcon_Click"/>
                            </Grid>
                        </Grid>
                    </Grid>
                    <Grid Grid.Column="1" HorizontalAlignment="Right" Margin="0,0,20,0">
                        <Button x:Name="NextButton" Visibility="Collapsed" Command="{Binding SearchNextCommand}" CommandParameter="{Binding Text, ElementName=PageSearchTxtBox}" Style="{StaticResource PdfViewerSearchDownStyle}" Tag="NextPage" />
                    </Grid>
                    <Grid Grid.Column="2" HorizontalAlignment="Left"  Margin="20,0,0,0">
                        <Button x:Name="PrevButton" Visibility="Collapsed" Command="{Binding SearchPreviousCommand}" CommandParameter="{Binding Text, ElementName=PageSearchTxtBox}" Style="{StaticResource PdfViewerSearchUpStyle}" Tag="Prev" />
                    </Grid>
                    <Grid Grid.Column="2" HorizontalAlignment="Left" VerticalAlignment="Center">
                        <TextBlock x:Name="NotFoundTextBlock" Foreground="White"  FontFamily="Segoe UI" FontSize="13" Visibility="Collapsed">We didn't find any results.</TextBlock>
                    </Grid>
                </Grid>
                <Grid Grid.Column="1" x:Name="Close">
                    <Button HorizontalAlignment="Right" Style="{StaticResource PdfViewerCloseStyle}" Tag="Close" Click="CloseSearch_Click"/>
                </Grid>
            </Grid>
        </Grid>

        <Grid Grid.Row="3"  VerticalAlignment="Bottom"  Background="#262726"  Margin="0,-2,0,0">
            <Grid x:Name="bottomAnnotationTab" Visibility="Collapsed">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="3*"></ColumnDefinition>
                    <ColumnDefinition Width="0.5*"></ColumnDefinition>
                    <ColumnDefinition Width="3*"></ColumnDefinition>
                    <ColumnDefinition Width="3*"></ColumnDefinition>
                    <ColumnDefinition Width="3*"></ColumnDefinition>
                    <ColumnDefinition Width="0.5*"></ColumnDefinition>
                    <ColumnDefinition Width="3*"></ColumnDefinition>
                    <ColumnDefinition Width="3*"></ColumnDefinition>
                    <ColumnDefinition Width="3*"></ColumnDefinition>
                    <ColumnDefinition Width="0.5*"></ColumnDefinition>
                    <ColumnDefinition Width="3*"></ColumnDefinition>
                    <ColumnDefinition Width="3*"></ColumnDefinition>
                    <ColumnDefinition Width="8*"></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <Grid Grid.Column="0" HorizontalAlignment="Center">
                    <ToggleButton Style="{StaticResource InkButtonStyle}" Name="InkButton" Tag="Ink" Checked="Ink_Checked" Unchecked="Ink_Unchecked"/>
                </Grid>
                <Rectangle Grid.Column="1" Height="40" Width="1" Margin="0,-12,0,0" Fill="White"/>
                <Grid Grid.Column="2" HorizontalAlignment="Center">
                    <ToggleButton Style="{StaticResource HighlightButtonStyle}" Name="HighlightButton" Tag="Highlight" Checked="Highlight_Checked" Unchecked="Highlight_Unchecked"/>
                </Grid>
                <Grid Grid.Column="3" HorizontalAlignment="Center">
                    <ToggleButton Style="{StaticResource UnderlineButtonStyle}" Name="UnderlineButton" Tag="Underline" Checked="UnderlineButton_Checked" Unchecked="UnderlineButton_Unchecked"/>
                </Grid>
                <Grid Grid.Column="4" HorizontalAlignment="Center">
                    <ToggleButton Style="{StaticResource StrikethroughButtonStyle}" Name="StrikethroughButton" Tag="Strikethrough" Checked="StrikethroughButton_Checked" Unchecked="StrikethroughButton_Unchecked"/>
                </Grid>
                <Rectangle Grid.Column="5" Height="40" Width="1" Margin="0,-12,0,0" Fill="White"/>
                <Grid Grid.Column="6" HorizontalAlignment="Center">
                    <ToggleButton Style="{StaticResource LineButtonStyle}" Name="LineButton" Tag="Line" Checked="LineButton_Checked" Unchecked="LineButton_Unchecked"/>
                </Grid>
                <Grid Grid.Column="7" HorizontalAlignment="Center">
                    <ToggleButton Style="{StaticResource RectangleButtonStyle}" Name="RectangleButton" Tag="Rectangle" Checked="RectangleButton_Checked" Unchecked="RectangleButton_Unchecked"/>
                </Grid>
                <Grid Grid.Column="8" HorizontalAlignment="Center">
                    <ToggleButton Style="{StaticResource EllipseButtonStyle}" Name="EllipseButton" Tag="Ellipse" Checked="EllipseButton_Checked" Unchecked="EllipseButton_Unchecked"/>
                </Grid>
                <Rectangle Grid.Column="9" Height="40" Width="1" Margin="0,-12,0,0" Fill="White"/>
                <Grid Grid.Column="10" HorizontalAlignment="Center">
                    <Button Style="{StaticResource UndoStyle}" Tag="Undo" Command="{Binding ElementName=pdfViewer, Path=UndoCommand, Mode=TwoWay}"/>
                </Grid>
                <Grid Grid.Column="11" HorizontalAlignment="Center">
                    <Button Style="{StaticResource RedoStyle}" Tag="Redo"  Command="{Binding ElementName=pdfViewer, Path=RedoCommand, Mode=TwoWay}"/>
                </Grid>
                <Grid Grid.Column="12" HorizontalAlignment="Right">
                    <Button Style="{StaticResource PdfViewerCloseStyle}" Name="CloseAnnotations" Tag="CloseAnnotation" Click="CloseAnnotations_Click"/>
                </Grid>
            </Grid>
        </Grid>
    </Grid>
</Page>
